<h1>Listing admins</h1>

Message => <%= @message %><br />
Message2 => <%= @message2 %><br />

<table>
  <tr>
    <th>Current generation</th>
    <th>Num of gene elements</th>
    <th>genes_per_generation</th>
    <th>Value set</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admins.each do |admin| %>
  <tr>
    <td><%= admin.current_generation %></td>
    <td><%= admin.num_of_gene_elements %></td>
    <td><%= admin.genes_per_generation %></td>
    <td><%= admin.value_set %></td>
    <td><%= link_to 'Show', admin %></td>
    <td><%= link_to 'Edit', edit_admin_path(admin) %></td>
    <td><%= link_to 'Destroy', admin, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

